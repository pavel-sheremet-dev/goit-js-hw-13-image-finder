{{#each this}}
  <li class='gallery__item'>
    <div class='photo-card' data-id='{{id}}'>
      <div class='photo-card__img-container'>
        <img class='photo-card__img' src='{{webformatURL}}' data-src='{{largeImageURL}}' alt='' />
      </div>

      <div class='stats'>
        <p class='stats-item'>
          <i class='material-icons'>thumb_up</i>
          {{likes}}
        </p>
        <p class='stats-item'>
          <i class='material-icons'>visibility</i>
          {{views}}
        </p>
        <p class='stats-item'>
          <i class='material-icons'>comment</i>
          {{comments}}
        </p>
        <p class='stats-item'>
          <i class='material-icons'>cloud_download</i>
          {{downloads}}
        </p>
      </div>
    </div>
  </li>
{{/each}}